;BIND data for cns.local
;
$TTL 604800
@ IN SOA cnsexus.cns.local. root.localhost. (
		1	; Serial
		604800		; Refresh
		86400		; Retry
		2419200		; Expire
		3600 )		; Negative cache TTL
;
@		IN NS cnsexus.cns.local.
; Registros Host
@		IN A	192.168.3.115
cnsexus		IN A	192.168.3.115

;Registros Alias
sitio1		IN CNAME	cnsexus.cns.local.
sitio2		IN CNAME	cnsexus.cns.local.
